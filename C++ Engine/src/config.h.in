
#ifndef CMAKE_CONFIG

// Are we building with debug (engine tools) or release (no engine tools)
#cmakedefine DEBUG_ENABLED
#cmakedefine DEBUG_ENABLED_BOOL @DEBUG_ENABLED@
// Engine version
#cmakedefine ENGINE_VERSION_MAJOR @ENGINE_VERSION_MAJOR@
#cmakedefine ENGINE_VERSION_MINOR @ENGINE_VERSION_MINOR@
#cmakedefine ENGINE_VERSION_PATCH @ENGINE_VERSION_PATCH@
// PATH variables
#cmakedefine BUILD_DIR @BUILD_DIR@
#cmakedefine RESOURCE_PATH @RESOURCE_PATH@
#cmakedefine PATH_SEPARATOR @PATH_SEPARATOR@

#define CMAKE_CONFIG
#endif